{
  "manifest_version": 3,

  "name": "Property Street View",
  "description": "Adds Street View popup to Rightmove property listings",
  "version": "1.0",

  "icons": {
    "128": "icon.png",
    "32": "icon-sm.png"
  },

  "content_scripts": [
      {
          "matches": ["https://www.rightmove.co.uk/*"],
          "js": ["js/content_script.js"]
      }
  ]
}
