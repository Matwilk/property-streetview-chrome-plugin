{
  "manifest_version": 3,

  "name": "Property Street View",
  "description": "Adds Street View popup to Rightmove property listings",
  "version": "1.0",

  "icons": {
    "128": "icon.png",
    "32": "icon-sm.png"
  },

  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["js/content_script.js"]
      }
  ],

  "background": {
    "service_worker": "js/background.js"
  }
}
